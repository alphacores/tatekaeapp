<h1>Listing paticipants</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Event</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% paticipants.each do |paticipant| %>
  <tr>
    <td><%= paticipant.id %></td>
    <td><%= paticipant.name %></td>
    <td><%= paticipant.event_id %></td>
    <td><%= link_to 'Show', paticipant %></td>
    <td><%= link_to 'Edit', edit_paticipant_path(paticipant) %></td>
    <td><%= link_to 'Destroy', paticipant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Paticipant', new_paticipant_path %>
